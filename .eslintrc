{
  "extends": [
    "airbnb",
    "plugin:react/recommended"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true,
      "experimentalObjectRestSpread": true
    }
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "globals": {
    "fetch": true,
  },
  "rules": {
    "indent": 0,
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [
          ".js"
        ]
      }
    ],
    "react/jsx-indent": [
      2,
      4
    ],
    "react/jsx-space-before-closing": 0,
    "global-require": 0,
    "no-console": 0
  },
  "plugins": [
    "react"
  ]
}
